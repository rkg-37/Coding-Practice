import java.util.*;
import java.lang.*;
public class pattern6{
    public static void solve(){
        for(int i=5;i>=1;i--){
            for(int j=1;j<=i;j++){
                System.out.print("*");
            }
            System.out.println();
        }
    }
    public static void main(String[] args){
        solve();
    }
}

