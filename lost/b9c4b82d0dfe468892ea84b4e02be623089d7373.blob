import java.util.*;
public class pattern12{
	public static void solve(){
        int n=6;
        int q=1;
		for(int i=1;i<5;i++){
			for(int j=1;j<=i;j++){
				System.out.print(j);
			}
            for(int k=n;k>=1;k--){
                System.out.print(" ");
            }

            for(int l=1;l<=q;l++){
                System.out.print(l);
            }
            q++;
            n-=2;

			System.out.println();
		}
	}
	public static void main(String[] args){
		solve();
	}
}



