import java.util.*;
import java.lang.*;
public class pattern2{
    public static void solve(){
        for(int i=1;i<=5;i++){
            for(int j=0;j<i;j++){
                System.out.print("*");
            }
            System.out.println();
        }
        
    }
    public static void main(String[] args){
        solve();
    }
}

