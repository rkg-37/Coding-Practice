import java.util.*;
import java.lang.*;
public class pattern3{
    public static void solve(){
        for(int i=1;i<=5;i++){
            for(int j=1;j<=i;j++){
                System.out.print(j);
            }
            System.out.println();
        }
    }
    public static void main(String[] args){
        solve();
    }
}

